<div class="buttons">
  <form method="GET" action="/memos/new">
    <input type="submit" value="新規作成">
  </form>
</div>

<ol>
  <% @memos&.each do |memo| %>
    <li>
      <a href="/memos/<%= memo["id"] %>"><%= CGI.escapeHTML(memo["title"].force_encoding("UTF-8")) %></a>
    </li>
  <% end %>
</ol>
